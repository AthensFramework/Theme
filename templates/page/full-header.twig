{% extends "page/base.twig" %}

{% block navheader %}
    <div id="header-container" class="full">
        <div id="header-container-inner" class="page-width">
            <div id="header-image">
            </div>
            <div id="header-wordmark">
                Athens
            </div>
            <div id="header-etc">
                Admissions / Registration / Graduation / Calendar
            </div>
        </div>

    </div>

    <div class="breadcrumbs-container">
        {% if breadCrumbs %}

            <nav class='breadcrumbs'><ul>
                    {% for crumbKey, crumbValue in breadCrumbs %}
                        {% if not loop.last %}
                            <li><a target="_self" href="{{ crumbValue }}">{{ crumbKey }}</a></li>
                        {% else %}
                            <li class="current"><span>{{ crumbValue }}</span></li>
                        {% endif %}
                    {% endfor %}

                </ul></nav>
        {% endif %}
    </div>
{% endblock navheader %}